<Window x:Class="BlackBoxTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlackBoxTest"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="350"
        Width="525">
    <StackPanel>
        <Button x:Name="BtnHello"
                Content="hello" />
        <Button x:Name="BtnWorld"
                Content="world" />
        <Button x:Name="BtnSession"
                Content="session" />
        <Button x:Name="BtnCaptureScreen"
                Content="capture screen" />
    </StackPanel>
</Window>
